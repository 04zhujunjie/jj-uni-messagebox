/*弹层动画（从下往上出现）*/
.fadelogIn.data-v-2387f610 {
    animation: fadelogIn-2387f610 .25s;
}
@keyframes fadelogIn-2387f610 {
0% {
        transform: translate3d(0, 100%, 0);
}
100% {
        transform: none;
}
}

/*弹层动画（从上往下消失）*/
.fadelogOut.data-v-2387f610 {
    animation: fadelogOut-2387f610 .25s;
	/* 保留动画结束后的样子 */
	animation-fill-mode: forwards;
	 -webkit-animation-fill-mode: forwards;
}
@keyframes fadelogOut-2387f610 {
0% {
       transform: none;
}
100% {
		transform: translate3d(0, 100%, 0);
}
}
